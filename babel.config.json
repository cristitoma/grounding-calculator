{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-modules-commonjs",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "~": "./src"
      }
    }]
  ]
}
